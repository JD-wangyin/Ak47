<assembly xmlns="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/plugins/maven-assembly-plugin/assembly/1.1.2 http://maven.apache.org/xsd/assembly-1.1.2.xsd">
  <id>assembly-boot</id>
  <formats>
    <format>jar</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  
  <fileSets>
    <fileSet>
      <outputDirectory>/</outputDirectory>
      
      <directory>target/classes/</directory>
      <includes>
        <include>com/wangyin/ak47/boot/*.class</include>
        <include>com/wangyin/ak47/common/Ak47Constants.class</include>
        <include>com/wangyin/ak47/common/Ak47Env.class</include>
        <include>com/wangyin/ak47/common/DynamicClassLoader.class</include>
      </includes>
    </fileSet>
    

  </fileSets>
  
  
</assembly>
